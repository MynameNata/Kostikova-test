%html
	%head
		%meta{:charset=>"utf-8"}
		%meta{:content => "width=device-width, initial-scale=1", :name => "viewport"}

		%title Test-UpQcode
		
		%link{:href => "css/bootstrap.css", :rel => "stylesheet", :type => "text/css"}
