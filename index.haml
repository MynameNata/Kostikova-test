	%body
		.container-fluid
			.row.header
				.header-menu.col-xs-16.col-sm-16.col-md-16.col-lg-12.col-lg-offset-2.col-md-offset-0.col-sm-offset-0.col-xs-offset-0

					-#logo
					.head-logo
						%a.header-logo{'href'=>'#','title'=>'logo'}

					-#menu	
					.head-menu
						%nav.menu_bar
							%li.menu_button
								%a.menu-link.black-color{'href'=>'#home','title'=>""} Home
								%ul.menu.top_menu
									%li.sub-list
										%a.menu_item{'href'=>'#','title'=>""} Name Surname
									%li.sub-list
										%a.menu_item{'href'=>'#','title'=>""} Name Surname
									%li.sub-list
										%a.menu_item{'href'=>'#','title'=>""} Name Surname
									%li.sub-list.sub_menu_container
										%a.menu_item{'href'=>'#','title'=>""} Name Surname
										%ul.menu.sub_menu
											%li.menu_item Name Surname
											%li.menu_item Name Surname
											%li.menu_item.sub_menu_container
												Name Surname
												%ul.menu.sub_menu
													%li.menu_item Name Surname
													%li.menu_item Name Surname

								
							%li.menu_button
								%a.menu-link.black-color{'href'=>'#products','title'=>""} Products
							%li.menu_button
								%a.menu-link.black-color{'href'=>'#about','title'=>""} About
							%li.menu_button
								%a.menu-link.black-color{'href'=>'#contacts','title'=>""} Contacts

					-#search
					.header-search-box.head-serch
						%form
							%lable{'for'=>'search'}
							%input#search.header-search{'type'=>'text'}


			-#slider
			#home.row.slider
					
				#carousel-example.carousel.slide{"data-ride" => "carousel"}
					-#Indicators
					%ol.carousel-indicators
						%li.active{"data-slide-to" => "0", "data-target" => "#carousel-example-generic"}
						%li{"data-slide-to" => "1", "data-target" => "#carousel-example-generic"}
						%li{"data-slide-to" => "2", "data-target" => "#carousel-example-generic"}
					-#Wrapper for slides
					.carousel-inner
						.item.active
							%img.bg-position{'src'=>'./img//slider-1.png'}
								 
						.item
							%img{:alt => "nature", :src => "http://lorempixel.com/1600/790/nature/2"}
						.item
							%img{:alt => "nature", :src => "http://lorempixel.com/1600/790/nature/3"}
					-#Controls
					%a.left.carousel-control.hidden-xs{"data-slide" => "prev", :href => "#carousel-example"}
						%span.slider-arrow.slider-arrow-left
					%a.right.carousel-control.hidden-xs{"data-slide" => "next", :href => "#carousel-example"}
						%span.slider-arrow.slider-arrow-right


